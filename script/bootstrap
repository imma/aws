#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"

  block compile pipenv36 "$@"
  local pth_aws="$(pipenv36 run which aws)"
  mkdir -p "$shome/bin"
  (set +f; cd bin && ln -nfs "$pth_aws" "$pth_aws"-* . )
}

main "$@"
