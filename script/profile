#!/usr/bin/env bash

# see https://docs.aws.amazon.com/cli/latest/topic/config-vars.html

function _aws_profile {
  local shome="${_aws_home:="$(cd -P -- "$(dirname -- "$BASH_SOURCE")/.." && pwd -P)"}"
  PATH="$shome/bin:$PATH"
  
  export AWS_DEFAULT_OUTPUT='json'
  AWS_VERSION='1.11.110'
}

_aws_profile
